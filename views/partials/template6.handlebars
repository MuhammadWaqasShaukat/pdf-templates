  <body>

    <div class="flex flex-col justify-center items-center">
      <div class="w-[3.5in] h-[2in] bg-black" id="side-1">
        <div class="grid place-items-center h-full w-full">
          <h1
            class="uppercase text-white font-bold tracking-wider text-lg flex items-center"
          >
            <img class="w-6 h-4 m-1" h-5 src="{{logo}}" alt="logo" />
            {{company}}
          </h1>
        </div>
      </div>
      <div
        class="w-[3.5in] h-[2in] bg-white p-5 relative overflow-hidden"
        id="side-2"
      >
        <img
          class="w-40 h-28 absolute top-[-25px] right-[-40px]"
          h-5
          src="{{logo}}"
          alt="background"
        />
        <div class="flex flex-col gap-4">
          <div class="w-full">
            <h2
              class="tracking-widest uppercase font-bold text-lg flex items-center"
            >
              <img
                class="w-6 h-4 m-1"
                h-5
                src="{{logo}}"
                alt="logo"
              />{{company}}
            </h2>
          </div>
          <div class="flex flex-col gap-3">
            <div class="flex-col flex gap-0">
              <h3 class="text-base font-bold uppercase tracking-wide">
                {{title}}
              </h3>
              <span
                class="text-[0.6rem] font-semibold uppercase text-[#f4951d]"
              >{{designation}}</span>
            </div>

            <!-- spacer -->
            <div class="h-[1px] w-full bg-gray-200"></div>

            <div class="grid grid-cols-2 grid-rows-2 gap-2">
              <div class="flex flex-row items-center gap-2">
                <div class="icon icon-phone h-3 w-3 bg-black rounded-sm"></div>
                <span class="text-[.5rem]">{{phone}}</span>
              </div>
              <div class="flex flex-row items-center gap-2">
                <div class="icon icon-globe h-3 w-3 bg-black rounded-sm"></div>
                <span class="text-[.5rem]">{{website}}</span>
              </div>
              <div class="flex flex-row items-center gap-2">
                <div class="icon icon-home h-3 w-3 bg-black rounded-sm"></div>
                <span class="text-[.5rem]">{{location}}</span>
              </div>
              <div class="flex flex-row items-center gap-2">
                <div
                  class="icon icon-envelop h-3 w-3 bg-black rounded-sm"
                ></div>
                <span class="text-[.5rem]">{{email}}</span>
              </div>
            </div>
          </div>
        </div>
      </div>
    </div>
  </body>
