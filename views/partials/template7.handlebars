          <p
            class="uppercase text-white font-normal tracking-wider text-lg pt-5 w-[50%]"
          >
            {{slogan}}
          </p>
          <img
            class="w-full h-full absolute top-0 right-[-45%] mr-auto opacity-20"
            src="{{logo1}}"
            alt="logo"
          />
        </div>
